{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "probe-rs-debug",
			"request": "launch",
			"name": "probe_rs Executable launch example",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "probe-rs",
			"runtimeArgs": [
				"dap-server"
			],
			//!MODIFY
			"chip": "STM32WB55CGUX",
			"flashingConfig": {
				"flashingEnabled": true,
				"haltAfterReset": false,
				"formatOptions": {
					"binaryFormat": "elf"
				}
			},
			"coreConfigs": [
				{
					"coreIndex": 0,
					"programBinary": "${workspaceFolder}/target/thumbv7em-none-eabihf/debug/tinylcd",
					"svdFile": "${workspaceFolder}/svd/STM32WB55_CM4.svd",
				}
			],
			"env": {
				// If you set this variable, check the VSCode console log window for the location of the log file.
				"RUST_LOG": "info"
			},
			// Info, Debug
			"consoleLogLevel": "Console"
		}
	]
}